{
    "env": {
        "es2021": true,
        "jest/globals": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 2],
        "no-unused-vars": ["warn"],
        "no-underscore-dangle": 0,
        "import/prefer-default-export": 0,
        "class-methods-use-this": ["warn"],
        "no-new": ["warn"]
    },
    "plugins": [
        "jest"
    ],
    "ignorePatterns": ["**/src/thirdparty/*"]
}
