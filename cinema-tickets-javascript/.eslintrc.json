{
    "env": {
        "es2021": true,
        "jest/globals": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 2]
    },
    "plugins": [
        "jest"
    ],
    "ignorePatterns": ["**/src/thirdparty/*"]
}
